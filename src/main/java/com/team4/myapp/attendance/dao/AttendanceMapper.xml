<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team4.myapp.attendance.dao.IAttendanceRepository">
	<select id="selectAttendanceId" parameterType="map" resultType="int">
		<![CDATA[
			SELECT ATTENDANCE_ID AS attendanceId
			FROM ATTENDANCE
			WHERE MEMBER_ID = #{memberId} and ATTENDANCE_DATE=#{attendanceDate}
		]]>
	</select>

</mapper>