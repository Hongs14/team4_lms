<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.team4.myapp.reasoncategory.dao.IReasonCategoryRepository">
	
	<select id="selectCategoryList" parameterType="map" resultType="com.team4.myapp.reasoncategory.model.ReasonCategory">
		<![CDATA[
			SELECT CATEGORY_ID AS categoryId,
					CATEGORY_NAME  AS categoryName
			FROM REASONCATEGORY
		]]>
	</select>
</mapper>